<DefaultWindow
    xmlns="https://spacestation14.io"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:gfx="clr-namespace:Robust.Client.Graphics;assembly=Robust.Client"
    x:Class="Content.Client.Interaction.Panel.Ui.InteractionEditorMenu"
    Title="{Loc 'interaction-editor-ui-default-title'}"
    MinSize="540 560"
    SetSize="540 560">

    <BoxContainer Orientation="Vertical" VerticalExpand="True" Margin="4">
        <Label StyleClasses="LabelBig" Text="{Loc 'interaction-label-big-settings-name'}" MinWidth="120"/>
        <BoxContainer Name="EditorContainer" Orientation="Vertical" Margin="2"/>
        <BoxContainer Orientation="Horizontal" Margin="4">
            <Button Name="EditButton" Text="{Loc 'interaction-button-edit-name'}" Disabled="False"/>
        </BoxContainer>
    </BoxContainer>

</DefaultWindow>
